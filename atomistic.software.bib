  author = {Sadooghi, Iman and Martin, Jesus Hernandez and Li, Tonglin and Brandstatter, Kevin and Maheshwari, Ketan and {de Lacerda Ruivo}, Tiago Pais Pitta and Garzoglio, Gabriele and Timm, Steven and Zhao, Yong and Raicu, Ioan},
  year = {2017},
