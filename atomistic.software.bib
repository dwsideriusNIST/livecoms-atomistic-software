  author = {{van de Sandt}, Stephanie and Nielsen, Lars Holm and Ioannidis, Alexandros and Muench, August and Henneken, Edwin and Accomazzi, Alberto and Bigarella, Chiara and Lopez, Jose Benito Gonzalez and {Dallmeier-Tiessen}, S{\"u}nje},
  year = {2019},
